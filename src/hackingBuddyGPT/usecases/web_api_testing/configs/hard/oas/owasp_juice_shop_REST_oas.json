{
  "openapi": "3.0.0",
  "servers": [
    {
      "url": "http://localhost:3000/rest"
    }
  ],
  "info": {
    "title": "Application API",
    "description": "API documentation for the application's REST and Web3 endpoints.",
    "version": "1.0.0"
  },
