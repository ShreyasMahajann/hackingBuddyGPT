      "get": {
        "summary": "Get basket by ID",
        "operationId": "getBasket",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Basket retrieved"
          }
        }
      }
